<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.loginregister">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Eventastic"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LoginRegister"
        android:usesCleartextTraffic="true">

        <activity android:name=".EGuestCrew.GuestCrewInsertGuest"
            android:theme="@style/Theme.Design.Light.NoActionBar" />

        <activity android:name=".EGuestCrew.GuestCrewInsertCrew"
            android:theme="@style/Theme.Design.Light.NoActionBar" />

        <activity
            android:name=".EGuestCrew.GuestCrew"
            android:label="@string/title_activity_guest_crew"
            android:theme="@style/Theme.Design.Light.NoActionBar" />
        <activity
            android:name=".Dashboard"
            android:theme="@style/Theme.Design.Light.NoActionBar" />
        <activity
            android:name=".FVenue.VenueActivity"
            android:theme="@style/Theme.Design.Light.NoActionBar" />
        <activity
            android:name=".FVenue.AddVenueDetail"
            android:theme="@style/Theme.Design.Light.NoActionBar" />
        <activity
            android:name=".CMainMenu.BeverageDetailActivity"
            android:theme="@style/Theme.Design.Light.NoActionBar" />
        <activity
            android:name=".DBooking.BookingActivity"
            android:theme="@style/Theme.Design.Light.NoActionBar" />
        <activity
            android:name=".DBooking.AddBookingDetail"
            android:theme="@style/Theme.Design.Light.NoActionBar" />
        <activity
            android:name=".ALoginRegister.Login"
            android:theme="@style/Theme.Design.Light.NoActionBar" />
        <activity
            android:name=".BCreateEvent.InsertEventDetails"
            android:theme="@style/Theme.Design.Light.NoActionBar" />
        <activity
            android:name=".ALoginRegister.SignUp"
            android:theme="@style/Theme.Design.Light.NoActionBar" />
        <activity
            android:name=".BCreateEvent.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Design.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>